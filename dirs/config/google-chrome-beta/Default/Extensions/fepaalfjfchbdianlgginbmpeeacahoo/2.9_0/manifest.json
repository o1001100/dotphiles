   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Toolbox for Google Play Store™",
   "options_ui": {
      "open_in_tab": true,
      "page": "settings.html"
   },
   "permissions": [ "*://play.google.com/*", "*://www.apkmirror.com/*", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
