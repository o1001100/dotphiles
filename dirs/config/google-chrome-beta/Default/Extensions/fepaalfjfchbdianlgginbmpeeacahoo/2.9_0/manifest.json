{
   "author": "APKMirror",
   "background": {
      "persistent": false,
      "scripts": [ "js/background.js" ]
   },
   "content_scripts": [ {
      "css": [ "css/styles.css" ],
      "js": [ "js/popper.min.js", "js/tippy-bundle.min.js", "js/regions.js", "js/toolbox.js" ],
      "matches": [ "*://play.google.com/*" ],
      "run_at": "document_idle"
   } ],
